<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="/style.css" />
  </head>
  <body>
    <h1>User Page</h1>

    <% users.forEach(user => { %>
    <section class="list">
      <a href="/users/<%= user._id %>">
        <h2><%= user.name %></h2>
      </a>
    </section>
    <% }) %>
  </body>
</html>
